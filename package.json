{
  "name": "labmanager",
  "version": "1.0.22",
  "author": "Angel Castro <me@angelcastro.info>",
  "description": "An application for managing a dental prosthetic laboratory",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": "https://github.com/alcastrob/labManager",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit",
    "unit": "karma start test/unit/karma.conf.js",
    "unit2": "mocha test/unit2 --recursive --require @babel/register",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src"
  },
  "build": {
    "productName": "labManager",
    "appId": "com.alcastro.labManager",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/tooth.ico"
    },
    "linux": {
      "icon": "build/icons"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "alcastrob",
        "repo": "labManager"
      }
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.10.3",
    "axios": "^0.18.1",
    "better-sqlite3": "^5.4.3",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "chart.js": "^2.9.3",
    "compare-version": "^0.1.2",
    "dropzone": "^5.7.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.3.1",
    "file-saverjs": "^1.3.6",
    "googleapis": "^48.0.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.8",
    "loglevel-server-send": "^1.0.0",
    "mime-types": "^2.1.27",
    "popper.js": "^1.16.1",
    "tableexport": "^5.2.0",
    "vue": "^2.6.11",
    "vue-clickaway": "^2.2.2",
    "vue-electron": "^1.0.6",
    "vue-events": "^3.1.0",
    "vue-floatthead": "0.0.5",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.3.4",
    "vue-scrollto": "^2.18.1",
    "vue-swal": "^1.0.0",
    "vue-toasted": "^1.1.28",
    "vuedraggable": "^2.23.2",
    "vuelidate": "^0.7.5",
    "vuex": "^3.4.0",
    "vuex-electron": "^1.0.3",
    "winston": "^2.4.5",
    "winston-electron": "^0.2.0",
    "winston-transport": "^4.4.0",
    "xlsx": "^0.14.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "accounting": "^0.4.1",
    "ajv": "^6.12.2",
    "babel-eslint": "^10.1.0",
    "cfonts": "^2.8.5",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.15",
    "electron-builder": "^20.44.4",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.11.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^8.0.1",
    "moment": "^2.27.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "printd": "^1.4.2",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.3.1",
    "should": "^13.2.3",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.11",
    "vuetable-2": "^1.7.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
